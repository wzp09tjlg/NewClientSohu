    android:layout_width="match_parent" android:layout_height="match_parent">
    <com.sina.home.testfor_newclient.widget.gridview.HorizontalGridView
        android:id="@+id/cnc_title_grid"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen_80dp"
        android:layout_centerVertical="true"
    />
    <com.sina.home.testfor_newclient.widget.TitleHorizontalScrollView.TitleHoriontalScrollView
